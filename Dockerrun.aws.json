{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions":
	[
		{
			"name": "client",
			"image": "bradwilder31415/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "server",
			"image": "bradwilder31415/multi-server",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "worker",
			"image": "bradwilder31415/multi-worker",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "bradwilder31415/multi-nginx",
			"essential": true,
			"portMappings":
			[
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links":
			[
				"client",
				"server"
			]
		}
	]
}